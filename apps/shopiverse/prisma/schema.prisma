generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("NUXT_DATABASE_URL")
}

model Products {
  id      String       @id @default(uuid())
  title   String
  description    String?
  image          String?
  category       String?
  price          Int
  created_at    DateTime?     @default(now())   @db.Timestamptz(6)
}
